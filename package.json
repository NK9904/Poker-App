{
  "name": "poker-app",
  "version": "1.0.0",
  "description": "AI poker solver with 3D assistant and optimized performance",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "vite-bundle-analyzer",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "perf": "npm run build && npm run preview & npm run lighthouse",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "setup": "node scripts/setup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "validate": "node scripts/validateDeployment.js",
    "predeploy": "npm run validate"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.0",
    "@react-three/drei": "^9.99.7",
    "@react-three/fiber": "^8.15.19",
    "autoprefixer": "^10.4.17",
    "clsx": "^2.1.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "three": "^0.162.0",
    "web-vitals": "^3.5.2",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse": "^12.2.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.6.3",
    "vite": "^7.1.2",
    "vite-bundle-analyzer": "^0.8.0",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-window": "^7.3.0"
  },
  "optionalDependencies": {
    "@types/lodash": "^4.14.202",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "puppeteer": "^24.16.2",
    "sqlite3": "^5.1.6"
  }
}
