{
  "name": "poker-app",
  "version": "1.0.0",
  "description": "AI poker solver with optimized performance",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "vite-bundle-analyzer",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "perf": "npm run build && npm run preview & npm run lighthouse",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "setup": "node scripts/setup.js",
    "scrape-data": "node src/scripts/scrapePokerData.js",
    "train-model": "node src/scripts/trainModel.js",
    "update-model": "node src/scripts/updateModel.js"
  },
  "dependencies": {
    "framer-motion": "^10.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "web-vitals": "^3.5.2",
    "zustand": "^4.5.7",
    "@deepseek-ai/deepseek-js": "^1.0.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.0.0",
    "node-cron": "^3.0.3",
    "sqlite3": "^5.1.6",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "tensorflow": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "onnxruntime-node": "^1.16.3",
    "lodash": "^4.17.21",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "lighthouse": "^12.2.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.6.3",
    "vite": "^7.1.2",
    "vite-bundle-analyzer": "^0.8.0",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-window": "^7.3.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0"
  }
}
