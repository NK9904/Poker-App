# DeepSeek AI Configuration
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Database Configuration (for data scraping)
DATABASE_URL=sqlite:./data/poker_data.db

# Scraping Configuration
SCRAPING_INTERVAL=0 */6 * * *  # Every 6 hours
MAX_CONCURRENT_SCRAPERS=3

# Model Training Configuration
TRAINING_BATCH_SIZE=4
TRAINING_EPOCHS=3
LEARNING_RATE_MULTIPLIER=0.1

# API Rate Limiting
DEEPSEEK_RATE_LIMIT=100  # requests per minute
SCRAPING_RATE_LIMIT=10   # requests per minute per domain

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/poker_ai.log

# Performance Configuration
CACHE_SIZE_LIMIT=1000
MODEL_CACHE_TTL=3600  # 1 hour in seconds

# Development Configuration
NODE_ENV=development
DEBUG=false